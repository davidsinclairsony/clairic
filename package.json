{
  "name": "interplay.app",
  "description": "consume media from the cloud on a browser",
  "author": "David Sinclair <david@sinclair.tech> (https://sinclair.tech)",
  "homepage": "https://interplay.app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "now-build": "next build",
    "start": "NODE_ENV=production node server.js"
  },
  "xo": {
    "prettier": true,
    "ignores": [
      "lib/localization"
    ],
    "extends": "xo-react",
    "rules": {
      "unicorn/filename-case": 0,
      "no-console": 2,
      "no-underscore-dangle": 2,
      "import/no-unassigned-import": 0
    },
    "env": [
      "browser",
      "node"
    ],
    "space": 2,
    "settings": {
      "import/core-modules": [
        "styled-jsx",
        "styled-jsx/css"
      ]
    }
  },
  "dependencies": {
    "@zeit/next-css": "1.0.1",
    "antd": "3.13.6",
    "array-move": "1.0.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-import": "1.11.0",
    "dropbox": "2.5.9",
    "eslint": "5.14.1",
    "eslint-config-xo": "0.26.0",
    "eslint-config-xo-react": "0.18.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "js-cookie": "2.2.0",
    "lodash": "4.17.11",
    "memoize-one": "5.0.0",
    "moment": "2.24.0",
    "next": "8.0.3",
    "next-redux-wrapper": "3.0.0-alpha.1",
    "path-match": "1.2.4",
    "polished": "3.0.0",
    "prettier": "1.16.4",
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-player": "1.9.3",
    "react-redux": "6.0.1",
    "react-virtualized": "9.21.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "screenfull": "4.0.1",
    "thenby": "1.3.0",
    "webpack-filter-warnings-plugin": "1.2.1",
    "xo": "0.24.0"
  }
}
