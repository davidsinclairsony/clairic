{
  "name": "interplay.app",
  "description": "consume media from the cloud on a browser",
  "author": "David Sinclair <david@sinclair.tech> (https://sinclair.tech)",
  "homepage": "https://interplay.app",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "dev": "next",
    "now-build": "next build",
    "start": "next start"
  },
  "xo": {
    "ignores": [
      "components/Player/index.js"
    ],
    "prettier": true,
    "extends": "xo-react",
    "rules": {
      "complexity": 0,
      "unicorn/filename-case": 0,
      "no-console": 2,
      "no-underscore-dangle": 2,
      "import/no-unassigned-import": 0,
      "react/no-danger": 0,
      "promise/no-return-wrap": 0
    },
    "env": [
      "browser"
    ],
    "space": 2
  },
  "dependencies": {
    "array-move": "^2.2.1",
    "babel-plugin-react-native-web": "^0.12.0",
    "dropbox": "2.5.9",
    "js-cookie": "2.2.0",
    "lodash": "4.17.11",
    "memoize-one": "5.0.0",
    "moment": "2.24.0",
    "next": "^9.1.1",
    "path-match": "1.2.4",
    "polished": "3.0.0",
    "prop-types": "15.7.2",
    "react": "^16.11.0",
    "react-beautiful-dnd": "^12.0.0",
    "react-dom": "^16.11.0",
    "react-native": "^0.61.5",
    "react-native-web": "^0.12.0",
    "react-player": "1.9.3",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "screenfull": "4.0.1",
    "thenby": "1.3.0",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "eslint-config-xo-react": "^0.23.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "xo": "^0.29.0"
  }
}
