{
	"name": "interplay.app",
	"description": "consume media from the cloud on a browser",
	"author": "David Sinclair <david@sinclair.tech> (https://sinclair.tech)",
	"homepage": "interplay.app",
	"aws": "interplayapp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"deploy": "./.env.sh && serverless deploy",
		"destroy": "./.env.sh && serverless remove",
		"eslint": "eslint \"./**/*.js\"",
		"export": "yarn copy; next build; next export -o build",
		"invalidate": "serverless invalidate",
		"watch": "yarn copy; node node_modules/parlor/dist/next-server.js",
		"prettier": "prettier --write \"./**/*.js\"",
		"server": "yarn copy; yarn export; cd build && python -m SimpleHTTPServer",
		"sync": "serverless sync",
		"copy": "mkdir -p static/css; cp node_modules/antd/dist/antd.min.css static/css/"
	},
	"xo": {
		"prettier": true,
		"ignores": [
			"lib/localization"
		],
		"extends": "xo-react",
		"rules": {
			"unicorn/filename-case": 0,
			"react/no-find-dom-node": 0
		},
		"env": ["browser", "node"]
	},
	"dependencies": {
		"@zeit/next-css": "^1.0.1",
		"antd": "^3.10.4",
		"babel-eslint": "8.0.1",
		"dropbox": "2.5.9",
		"eslint": "^5.8.0",
		"eslint-config-xo": "^0.25.0",
		"eslint-config-xo-react": "^0.17.0",
		"eslint-plugin-prettier": "^3.0.0",
		"eslint-plugin-react": "^7.11.1",
		"file-loader": "^2.0.0",
		"js-cookie": "^2.2.0",
		"lodash": "4.17.5",
		"memoize-one": "^4.0.2",
		"moment": "2.22.0",
		"next": "^7.0.2",
		"next-redux-wrapper": "^2.0.0",
		"parlor": "^0.1.1",
		"path-match": "1.2.4",
		"polished": "1.9.2",
		"prettier": "^1.14.2",
		"prop-types": "15.6.0",
		"query-parse": "2.0.0",
		"react": "16.3.1",
		"react-art": "^16.5.2",
		"react-beautiful-dnd": "^8.0.7",
		"react-dom": "16.3.1",
		"react-intl": "^2.7.1",
		"react-player": "^1.6.6",
		"react-redux": "5.0.7",
		"react-virtualized": "9.18.5",
		"redux": "3.7.2",
		"redux-thunk": "^2.2.0",
		"screenfull": "^3.3.3",
		"serverless": "1.25.0",
		"styled-jsx": "^3.1.0",
		"thenby": "1.2.3",
		"url-loader": "^1.1.2",
		"xo": "^0.23.0"
	}
}
