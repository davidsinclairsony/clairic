{
  "name": "interplay.app",
  "description": "consume media from the cloud on a browser",
  "author": "David Sinclair <david@sinclair.tech> (https://sinclair.tech)",
  "homepage": "https://interplay.app",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "now-build": "next build",
    "start": "NODE_ENV=production node server.js"
  },
  "xo": {
    "prettier": true,
    "ignores": [
      "lib/localization"
    ],
    "extends": "xo-react",
    "rules": {
      "unicorn/filename-case": 0,
      "no-console": 2,
      "no-underscore-dangle": 2,
      "import/no-unassigned-import": 0
    },
    "env": [
      "browser",
      "node"
    ],
    "space": 2,
    "settings": {
      "import/core-modules": [
        "styled-jsx",
        "styled-jsx/css"
      ]
    }
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.9",
    "@emotion/core": "^10.0.10",
    "@zeit/next-css": "^1.0.1",
    "array-move": "1.0.0",
    "babel-plugin-import": "1.11.0",
    "dropbox": "2.5.9",
    "js-cookie": "2.2.0",
    "lodash": "4.17.11",
    "memoize-one": "5.0.0",
    "moment": "2.24.0",
    "next": "^8.0.4",
    "path-match": "1.2.4",
    "polished": "3.0.0",
    "prop-types": "15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-player": "1.9.3",
    "react-virtualized": "9.21.0",
    "screenfull": "4.0.1",
    "thenby": "1.3.0",
    "webpack-filter-warnings-plugin": "1.2.1"
  },
  "devDependencies": {
    "eslint-config-xo-react": "^0.19.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "xo": "^0.24.0"
  }
}
