{
		"name": "mediafurry.com",
		"description": "consume media from the cloud on a browser",
		"author": "David Sinclair <david@sinclair.tech> (https://sinclair.tech)",
		"homepage": "mediafurry.com",
		"aws": "mediafurrycom",
		"version": "0.0.1",
		"private": true,
		"scripts": {
				"deploy": "serverless deploy",
				"destroy": "serverless remove",
				"eslint": "eslint \"./**/*.js\"",
				"export": "next build && next export -o build",
				"invalidate": "serverless invalidate",
				"next": "node server.js",
				"prettier": "prettier --write \"./**/*.js\"",
				"server": "yarn export; cd build && python -m SimpleHTTPServer",
				"sync": "serverless sync"
		},
		"dependencies": {
				"antd": "3.4.0",
				"babel-eslint": "8.0.1",
				"bluebird": "3.5.1",
				"dropbox": "2.5.9",
				"eslint": "4.11.0",
				"eslint-config-airbnb": "16.1.0",
				"eslint-config-prettier": "2.8.0",
				"eslint-plugin-import": "2.8.0",
				"eslint-plugin-jsx-a11y": "6.0.2",
				"eslint-plugin-react": "7.5.1",
				"lodash": "4.17.5",
				"moment": "2.22.0",
				"next": "5.1.0",
				"next-redux-wrapper": "1.3.5",
				"node-sass": "4.8.3",
				"nprogress": "0.2.0",
				"path-match": "1.2.4",
				"polished": "1.9.2",
				"prettier": "1.11.1",
				"prop-types": "15.6.0",
				"query-parse": "2.0.0",
				"react": "16.3.1",
				"react-dom": "16.3.1",
				"react-player": "^1.4.0",
				"react-redux": "5.0.7",
				"react-virtualized": "9.18.5",
				"react-window-size-listener": "1.2.1",
				"redux": "3.7.2",
				"redux-observable": "0.18.0",
				"redux-thunk": "2.2.0",
				"rxjs": "5.5.8",
				"serverless": "1.25.0",
				"styled-jsx": "2.2.6",
				"styled-jsx-plugin-sass": "0.2.4",
				"thenby": "1.2.3"
		}
}
